{
	"name": "@aicacia/svelte-editor",
	"version": "0.1.0",
	"description": "aicacia svelte rich text editor",
	"license": "(MIT OR Apache-2.0)",
	"type": "module",
	"keywords": [
		"svelte",
		"rich",
		"text",
		"editor"
	],
	"scripts": {
		"start": "svelte-kit dev",
		"build": "svelte-kit build",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"clean": "rm -rf node_modules/.vite",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"publish": "npm run package && cd package && npm publish",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "next",
		"@sveltejs/kit": "next",
		"@types/is-hotkey": "0",
		"@types/katex": "0",
		"@types/prismjs": "1",
		"@typescript-eslint/eslint-plugin": "5",
		"@typescript-eslint/parser": "5",
		"eslint": "8",
		"eslint-config-prettier": "8",
		"eslint-plugin-svelte3": "3",
		"prettier": "2",
		"prettier-plugin-svelte": "2",
		"slate": "0.72",
		"slate-history": "0",
		"svelte": "3",
		"svelte-check": "2",
		"svelte-icons": "2",
		"svelte-preprocess": "4",
		"svelte2tsx": "0",
		"tslib": "2",
		"typescript": "4"
	},
	"peerDependencies": {
		"slate": "0.72",
		"slate-history": "0",
		"svelte": "3",
		"svelte-icons": "2",
		"svelte-slate": "0.1"
	},
	"dependencies": {
		"image-extensions": "1.1",
		"is-url": "1.2",
		"katex": "0.15",
		"prismjs": "1.26",
		"scroll-into-view-if-needed": "2.2",
		"svelte-portal": "2.1",
		"svelte-slate": "0.1"
	}
}
